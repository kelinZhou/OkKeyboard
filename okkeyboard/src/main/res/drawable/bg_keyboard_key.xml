<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_keyboard_bg" />
        </shape>
    </item>
    <item android:left="2dp" android:right="2dp" android:top="4dp" android:bottom="3dp">
        <selector>
            <item android:state_pressed="false">
                <shape android:shape="rectangle">
                    <corners android:radius="5dp" />
                    <solid android:color="@color/color_key_button" />
                    <stroke
                        android:width="0.5dp"
                        android:color="@color/color_key_stroke" />
                </shape>
            </item>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <corners android:radius="5dp" />
                    <solid android:color="@color/color_key_button_pressed" />
                    <stroke
                        android:width="0.5dp"
                        android:color="@color/color_key_stroke_pressed" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>